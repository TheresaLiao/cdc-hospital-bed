<panel xmlns:n="native" title="心率" border="normal" height="100%" >
    <panelchildren>

 		<bs-row>
            <n:div class="col-lg-1">
                From
            </n:div>
            <n:div class="col-lg-2">
                <datebox hflex="1"/>
            </n:div>
            <n:div class="col-lg-1">
                To
            </n:div>
            <n:div class="col-lg-2">
                <datebox hflex="1"/>
            </n:div>
            <n:div class="col-lg-1">
                <button label="查詢"/>
            </n:div>
         </bs-row>
         <window apply="org.itri.view.humanhealth.detail.HeartbeatView">
			<charts id="chart" type="spline" marginRight="10" title="心率" />
    		<timer id="timer" delay="1000" running="true" repeats="true" />
		</window>
	</panelchildren>
</panel>