<zk xmlns:n="native">
    <nodom viewModel="@id('vm')@init('org.itri.view.humanhealth.PersonInfos')">
       <forEach items="@init(vm.states)">
            <n:div sclass="col-md-12 p-1">
                <person-info state="@init(each)"
                           icon="@init(each.type)@converter('org.zkoss.admin.converter.TypeIconConverter')"
                           color="@init(each.type)@converter('org.zkoss.admin.converter.TypeColorConverter')"
                           statutIcon="@init(each.status)@converter('org.itri.converter.StatusIconConverter')"/>
            </n:div>
        </forEach>
    </nodom>
</zk>